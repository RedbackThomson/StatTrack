<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:ViewModels="clr-namespace:StatTrack.UI.ViewModels"
                    xmlns:views="clr-namespace:StatTrack.UI.Views">

    <DataTemplate DataType="{x:Type ViewModels:OptionsViewModel}">
        <Grid>
            <views:OptionsView />
        </Grid>
    </DataTemplate>
    <DataTemplate DataType="{x:Type ViewModels:GraphViewModel}">
        <Grid>
            <views:GraphView />
        </Grid>
    </DataTemplate>
</ResourceDictionary>